{
  "$schema": "http://json-schema.org/schema#",
  "$ref": "#/$defs/innsettelsesordreType",
  "$defs": {
    "beslutningType": {
      "title": "beslutning",
      "type": "object",
      "required": ["besluttetAv", "tidspunkt"],
      "additionalProperties": false,
      "properties": {
        "besluttetAv": {
          "type": "string"
        },
        "tidspunkt": {
          "type": "string",
          "format": "date-time"
        },
        "restriksjoner": {
          "type": "array",
          "title": "restriksjoner",
          "items": {
            "type": "object",
            "required": ["kode", "lovreferanse"],
            "additionalProperties": false,
            "properties": {
              "kode": { "type": "string" },
              "lovreferanse": { "type": "string" },
              "startDato": { "type": "string", "format": "date" },
              "sluttDato": { "type": "string", "format": "date" }
            }
          }
        },
        "merknad": {
          "type": "string"
        }
      }
    },
    "kodeverkType": {
      "type": "object",
      "required": ["navn"],
      "additionalProperties": false,
      "properties": {
        "kode": {
          "type": "string"
        },
        "navn": {
          "type": "string"
        }
      }
    },
    "personFullType": {
      "type": "object",
      "required": ["internId", "rolle", "navn", "kjoenn", "foedselsdato"],
      "additionalProperties": false,
      "properties": {
        "internId": {
          "type": "number"
        },
        "rolle": {
          "$ref": "#/$defs/kodeverkType"
        },
        "navn": {
          "title": "navn",
          "type": "object",
          "required": ["fornavn", "etternavn"],
          "additionalProperties": false,
          "properties": {
            "fornavn": {
              "type": "string"
            },
            "etternavn": {
              "type": "string"
            }
          }
        },
        "foedselsnummer": {
          "type": "array",
          "description": "flere fødselsnummer mulig senere",
          "items": { "type": "string" }
        },
        "dNummer": {
          "type": "string"
        },
        "sspNummer": {
          "type": "string"
        },
        "kjoenn": {
          "type": "string"
        },
        "foedselsdato": {
          "type": "string"
        },
        "adresse": {
          "type": "string"
        },
        "telefonnumer": {
          "type": "string"
        },
        "statsborgerskap": {
          "type": "array",
          "title": "statsborgerskap",
          "items": {
            "$ref": "#/$defs/kodeverkType"
          }
        },
        "verge": {
          "type": "string"
        },
        "forsvarer": {
          "type": "string"
        }
      }
    },
    "personEnkelType": {
      "type": "object",
      "required": ["internId", "rolle", "navn"],
      "additionalProperties": false,
      "properties": {
        "internId": {
          "type": "number"
        },
        "rolle": {
          "$ref": "#/$defs/kodeverkType"
        },
        "navn": {
          "title": "navn",
          "type": "object",
          "required": ["fornavn", "etternavn"],
          "additionalProperties": false,
          "properties": {
            "fornavn": {
              "type": "string"
            },
            "etternavn": {
              "type": "string"
            }
          }
        },
        "kjoenn": {
          "type": "string"
        },
        "foedselsdato": {
          "type": "string"
        },
        "statsborgerskap": {
          "type": "array",
          "title": "statsborgerskap",
          "items": {
            "$ref": "#/$defs/kodeverkType"
          }
        }
      }
    },
    "adresseType": {
      "title": "adresse",
      "type": "object",
      "required": ["adresseLinjer", "postnummer", "poststed", "land"],
      "additionalProperties": false,
      "properties": {
        "adresseLinjer": {
          "type": "array",
          "title": "adresseLinjer",
          "items": {
            "type": "string"
          }
        },
        "postnummer": {
          "type": "string"
        },
        "poststed": {
          "type": "string"
        },
        "land": {
          "$ref": "#/$defs/kodeverkType"
        }
      }
    },
    "hendelseType": {
      "title": "hendelse",
      "type": "object",
      "required": [
        "gjerningstidspunktFra",
        "gjerningstidspunktTil",
        "gjerningssted"
      ],
      "additionalProperties": false,
      "properties": {
        "gjerningstidspunktFra": {
          "type": "string",
          "format": "date-time"
        },
        "gjerningstidspunktTil": {
          "type": "string",
          "format": "date-time"
        },
        "gjerningssted": {
          "$ref": "#/$defs/adresseType"
        }
      }
    },
    "straffesakType": {
      "title": "straffesak",
      "type": "object",
      "required": [
        "straffesaksnummer",
        "hendelse",
        "statistikkgruppe",
        "krimtype"
      ],
      "additionalProperties": false,
      "properties": {
        "straffesaksnummer": {
          "type": "string"
        },
        "hendelse": {
          "$ref": "#/$defs/hendelseType"
        },
        "statistikkgruppe": {
          "$ref": "#/$defs/kodeverkType"
        },
        "krimtype": {
          "$ref": "#/$defs/kodeverkType"
        },
        "prinsipalLovbud": {
          "title": "prinsipalLovbud",
          "type": "object",
          "required": ["lovreferanse", "gjengivelse"],
          "additionalProperties": false,
          "properties": {
            "lovbudId": {
              "type": "string"
            },
            "lovreferanse": {
              "type": "string"
            },
            "gjengivelse": {
              "type": "string"
            },
            "grunnlagstekst": {
              "type": "string"
            },
            "strafferamme": {
              "description": "antall måneder",
              "type": "number"
            }
          }
        },
        "involverte": {
          "title": "involverte",
          "description": "Siktede, domfelte, vitner og fornærmet i straffesaken",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "medsiktede": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/personFullType"
              }
            },
            "vitner": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/personEnkelType"
              }
            },
            "fornaermede": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/personEnkelType"
              }
            }
          }
        }
      }
    },
    "personVaretektInfoType": {
      "title": "personVaretektInfo",
      "type": "object",
      "required": ["personVaretekt", "straffesaker"],
      "additionalProperties": false,
      "properties": {
        "personVaretekt": {
          "$ref": "#/$defs/personFullType"
        },
        "helseopplysninger": {
          "type": "string"
        },
        "straffesaker": {
          "type": "array",
          "title": "straffesaker",
          "items": {
            "$ref": "#/$defs/straffesakType"
          }
        }
      }
    },
    "orgenhetType": {
      "title": "organisasjonsnummer",
      "type": "object",
      "required": ["orgnr"],
      "additionalProperties": false,
      "properties": {
        "orgnr": {
          "type": "string"
        },
        "navn": {
          "type": "string"
        }
      }
    },
    "avsenderInfoType": {
      "title": "avsenderinformasjon",
      "type": "object",
      "required": ["orgenhet", "saksbehandler"],
      "additionalProperties": false,
      "properties": {
        "orgenhet": {
          "$ref": "#/$defs/orgenhetType"
        },
        "saksbehandler": {
          "type": "object",
          "required": ["navn"],
          "additionalProperties": false,
          "properties": {
            "brukeridentifikasjon": { "type": "string" },
            "navn": { "type": "string" }
          }
        }
      }
    },
    "innsettelsesordreType": {
      "description": "JSON Schema generated by XMLSpy v2022 rel. 2 (x64) (http://www.altova.com)",
      "type": "object",
      "required": [
        "forsendelse",
        "hovedStraffesaksnummer",
        "kontaktPersonKDI",
        "varetektSyklusId",
        "kravId",
        "beslutning",
        "paagrepetTidspunkt",
        "personVaretektInfo"
      ],
      "additionalProperties": false,
      "properties": {
        "forsendelse": {
          "title": "forsendelse",
          "type": "object",
          "required": ["meldingsId", "avsender", "mottaker"],
          "additionalProperties": false,
          "properties": {
            "meldingsId": {
              "type": "string"
            },

            "avsender": {
              "$ref": "#/$defs/avsenderInfoType"
            },
            "mottaker": {
              "type": "object",
              "required": ["orgnr"],
              "additionalProperties": false,
              "properties": {
                "orgnr": { "type": "string" },
                "navn": { "type": "string" }
              }
            }
          }
        },
        "hovedStraffesaksnummer": { "type": "string" },
        "kontaktPersonKDI": {
          "$ref": "#/$defs/kodeverkType"
        },
        "varetektSyklusId": {
          "type": "string"
        },
        "kravId": {
          "type": "string"
        },
        "beslutning": {
          "$ref": "#/$defs/beslutningType"
        },
        "paagrepetTidspunkt": {
          "type": "string",
          "format": "date-time"
        },
        "personVaretektInfo": {
          "$ref": "#/$defs/personVaretektInfoType"
        },
        "paategningsdokumentRef": {
          "type": "string"
        },
        "dokumenter": {
          "type": "string"
        }
      }
    }
  }
}
